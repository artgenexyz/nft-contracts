contract Artgene721Factory {
    function create(
        string memory _name,
        string memory _symbol,
        uint256 _maxSupply,
        uint256 _nReserved,
        bool _startAtOne,
        string memory _uri,
        MintConfig memory _config
    ) external returns (Artgene721EmptyConstructor) {
        Artgene721EmptyConstructor instance = new Artgene721EmptyConstructor();

        instance.initialize(
            _name,
            _symbol,
            _maxSupply,
            _nReserved,
            _startAtOne,
            _uri,
            _config
        );

        instance.transferOwnership(msg.sender);

        return instance;
    }
}
